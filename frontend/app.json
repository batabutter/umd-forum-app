{
  "expo": {
    "name": "umd-course-forum",
    "slug": "umd-course-forum",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "umdcourseforum",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/umd-logo.png"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.myname.mycoolapp",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9494d5bf-4f0d-4cc8-b1a8-c14f7b1eb84d"
      }
    }
  }
}